<%
String result1=request.getParameter("result");
String answer1=request.getParameter("answer");
int rt=Integer.parseInt(result1);
int as=Integer.parseInt(answer1);
String msg="";

if(rt==as){
	msg="정답입니다";
}else(rt>as){
	msg="Down";
}else{
	msg="Up";
}

%>
<h1><%=msg %></h1>
<a href="index.jsp">[처음으로]</a>
</body>
